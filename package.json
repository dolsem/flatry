{
  "name": "flatry",
  "version": "1.0.5",
  "description":
    "Flatry converting promise or function to flat array response. Inspired by golang style error handling without try/catch.",
  "keywords": [
    "flat",
    "flatten",
    "promise",
    "async",
    "await",
    "golang",
    "error handling",
    "destructuring",
    "try",
    "catch"
  ],
  "main": "index.js",
  "scripts": {
    "minify": "uglifyjs index.js --compress --mangle -o index.min.js",
    "test": "jest --maxWorkers 4 && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymatuhin/flatry.git"
  },
  "author": "ymatuhin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ymatuhin/flatry/issues"
  },
  "homepage": "https://github.com/ymatuhin/flatry#readme",
  "devDependencies": {
    "codecov": "^3.0.0",
    "jest": "^21.2.1",
    "uglify-js": "^3.1.9"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
